<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:element name="vehicle" type="vehicle"/>

    <xs:complexType name="vehicle">
        <xs:sequence>
            <xs:element ref="vehicle" minOccurs="0"/>
            <xs:element name="id" type="xs:int" minOccurs="0"/>
            <xs:element name="vehicle_name" type="xs:string" minOccurs="0"/>
            <xs:element name="vehicle_status" type="xs:string" minOccurs="0"/>
            <xs:element name="empid" type="xs:int" minOccurs="0"/>
            <xs:element name="orders_order_id" type="xs:int" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>